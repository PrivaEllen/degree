{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "prettier.enable": true,
    "eslint.enable": true,
    "eslint.alwaysShowStatus": true,
    "typescript.updateImportsOnFileMove.enable": "always",
    "javascript.preference.importModuleSpecifier": "relative",
    "javascript.updateImportsOnFileMove.enable": "always",
    "typescript.preference.importModuleSpecifier": "relative",
    "files.eol": "\n"
}
